test256:
	head -c 32 /dev/urandom > data256.random
	openssl aes-256-cbc -in data256.random -out data256.openssl -K 000102030405060708090a0b0c0d0e0f101112131415161718191a1b1c1d1e1f -iv 00000000000000000000000000000000
	sudo python client.py -in data256.random -out data256.fpga -K 000102030405060708090a0b0c0d0e0f101112131415161718191a1b1c1d1e1f -iv 00000000000000000000000000000000
	diff data256.openssl data256.fpga

